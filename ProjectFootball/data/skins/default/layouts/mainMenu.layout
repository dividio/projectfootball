        <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
        <Property Name="Image" Value="set:Background image:Background" />
        <Property Name="FrameEnabled" Value="false" />
        <Window Type="TaharezLook/Button" Name="MainMenu/QuickPlayButton" >
            <Property Name="Font" Value="DejaVuSans-10" />
            <Property Name="Text" Value="Quick Play" />
            <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
            <Property Name="UnifiedAreaRect" Value="{{0.35,0},{0.25,0},{0.65,0},{0.30,0}}" />
            <Event Name="Clicked" Function="cmd_Menu_QuickPlay" />
        </Window>
        <Window Type="TaharezLook/Button" Name="MainMenu/LoadGameButton" >
            <Property Name="Font" Value="DejaVuSans-10" />
