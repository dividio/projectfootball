        </Window>
        <Window Type="TaharezLook/Button" Name="MainMenu/QuitButton" >
            <Property Name="Font" Value="DejaVuSans-10" />
            <Property Name="Text" Value="Quit" />
            <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
            <Property Name="UnifiedAreaRect" Value="{{0.35,0},{0.70,0},{0.65,0},{0.75,0}}" />
            <Event Name="Clicked" Function="cmd_Menu_Quit" />
        </Window>
    </Window>
</GUILayout>
